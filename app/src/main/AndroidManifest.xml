<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cecilia.FeelsBook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ywei2-FeelsBook"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="ywei2-FeelsBook"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddRecordActivity"
            android:label="New record" />
        <activity android:name=".ViewRecordActivity"></activity>
        <activity android:name=".ViewCategoryActivity"></activity>
        <activity android:name=".EditRecordActivity"
            android:label="Edit record"/>

    </application>

</manifest>